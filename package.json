{
  "name": "@open-audio-stack/registry",
  "version": "0.0.2",
  "description": "Audio registry with searchable list of packages containing Plugins, Presets and Projects",
  "type": "module",
  "main": "./build/index.js",
  "exports": {
    "node": "./build/index.js",
    "default": "./build/index-browser.js"
  },
  "types": "./build/index.d.ts",
  "files": [
    "build"
  ],
  "scripts": {
    "copy": "cp -rf ./src/plugins ./out && cp -rf ./src/presets ./out && cp -rf ./src/projects ./out",
    "build": "tsc -p tsconfig.json",
    "dev": "tsx ./src/index.ts",
    "format": "prettier . --write",
    "lint": "eslint .",
    "start": "node build/index.js && npm run copy",
    "test": "vitest run --config vitest.config.ts",
    "test:api": "playwright test --debug",
    "test:watch": "vitest ./tests"
  },
  "author": "kmturley",
  "license": "cc0-1.0",
  "keywords": [
    "open",
    "audio",
    "stack",
    "plugin",
    "package",
    "manager",
    "registry",
    "search",
    "metadata"
  ],
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "@playwright/test": "^1.48.2",
    "@types/node": "^22.7.8",
    "@vitest/coverage-v8": "^2.1.4",
    "eslint": "^9.12.0",
    "globals": "^15.2.0",
    "playwright": "1.46",
    "prettier": "^3.2.5",
    "tsx": "^4.10.1",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.9.0",
    "vitest": "^2.1.4"
  },
  "dependencies": {
    "@open-audio-stack/core": "^0.0.5",
    "chalk": "^5.3.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/open-audio-stack/open-audio-stack-registry.git"
  }
}
